<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.bsa.controllers.CancelledAppointmentsController">
   <children>
      <ImageView fitHeight="414.0" fitWidth="618.0" pickOnBounds="true">
         <image>
            <Image url="@background_design.png" />
         </image>
      </ImageView>
      <TableView fx:id="CancelledAppsTableView" layoutX="64.0" layoutY="120.0" prefHeight="200.0" prefWidth="515.0">
        <columns>
          <TableColumn fx:id="date" prefWidth="118.0" text="Date" />
          <TableColumn fx:id="service" prefWidth="320.0" text="Services" />
            <TableColumn fx:id="freeTime" prefWidth="76.0" text="Free Time" />
        </columns>
      </TableView>
      <Label layoutX="173.0" layoutY="66.0" text="Cancelled Appointments">
         <font>
            <Font name="Lucida Calligraphy Italic" size="21.0" />
         </font>
      </Label>
      <Label layoutX="120.0" layoutY="339.0" text="If you want to free time for an appointment, please press 'Free time' Button" />
      <Button fx:id="backtoAppts" layoutX="14.0" layoutY="41.0" mnemonicParsing="false" onAction="#handleToAppointmentsFromCancelled" text="&lt;&lt; APPOINTMENTS" />
   </children>
</AnchorPane>
